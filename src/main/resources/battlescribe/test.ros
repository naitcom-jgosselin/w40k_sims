            <profile id="fb5a-2f31-b730-9b32::c0c4-a119-9fe4-401f" name="Masterwork Bionics" publicationId="ebe8-544e-pubN65537" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
